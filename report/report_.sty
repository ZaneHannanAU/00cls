% Old stuff

\makeatletter
\renewcommand\tableofcontents{%
	\onecolumn%
	\section*{\contentsname}\addcontentsline{toc}{chapter}{\contentsname}%

	\@mkboth{\MakeUppercase\contentsname}%
		{\MakeUppercase\contentsname}%
	\@starttoc{toc}%
	\ifAfour\twocolumn\fi
}
\renewcommand\listoftables{%
	\onecolumn%
	\chapter{\listtablename}%

	\@mkboth{\MakeUppercase\listtablename}%
		{\MakeUppercase\listtablename}%
	\@starttoc{lot}%
	\ifAfour\twocolumn\fi
}
\renewcommand\listoffigures{%
	\onecolumn%
	\chapter{\listfigurename}%

	\@mkboth{\MakeUppercase\listfigurename}%
		{\MakeUppercase\listfigurename}%
	\@starttoc{lof}%
	\ifAfour\twocolumn\fi
}

% Frontmatter, Mainmatter and Backmatter
\newcommand\frontmatter{%
	\if@titlepage
	\else
		\cleardoublepage
	\fi
	%\@mainmatterfalse
	\pagenumbering{roman}
}

\newcommand\mainmatter{%
	\cleardoublepage
	% \@mainmattertrue
	\pagenumbering{arabic}
}

\newcommand\backmatter{%
	\appendix
	\cleardoublepage
	% \@mainmatterfalse
	\pagenumbering{Alph}
	\phantomsection\addcontentsline{toc}{part}{Appendicies}
}

% non-toc
\newcommand\listofall{%
	\listoftables%
	\listoffigures%
}
\makeatother

